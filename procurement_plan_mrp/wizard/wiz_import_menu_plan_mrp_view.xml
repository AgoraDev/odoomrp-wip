<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="wiz_import_menu_plan_mrp_view" model="ir.ui.view">
            <field name="name">wiz.import.menu.plan.mrp.view</field>
            <field name="model">wiz.import.menu.plan.mrp</field>
            <field name="arch" type="xml">
                <form string="Procurements">
                    <group string="Import Procurements Menu">
                        <label string="You must select one of the two import options" class="oe_inline" />
                    </group>
                    <footer>
                        <button name="do_import_procurements" type="object" string="Import ALL Procurements" class="oe_highlight" />
                        or
                        <button name="do_import_procurements_internal" type="object" string="Import Procurements WITH INTERNAL Location" class="oe_highlight" />
                        or
                        <button string="Cancel" class="oe_link" special="cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</openerp>
