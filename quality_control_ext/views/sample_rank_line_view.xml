<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<!-- *********************** VISTA TREE ****************** -->

		<record model="ir.ui.view" id="sample_rank_line_tree_view">
			<field name="name">sample.rank.line.tree.view</field>
			<field name="model">sample.rank.line</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Sample Ranks Lines" editable="bottom" >
					<field name="fromm" />
					<field name="to" />
					<field name="how_many" />
				</tree>
			</field>
		</record>
		
		<!-- ******************* VISTA FORM ******************** -->

		<record model="ir.ui.view" id="sample_rank_line_form_view">
			<field name="name">sample.rank.line.form.view</field>
			<field name="model">sample.rank.line</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Sample Rank Line">
					<group colspan="4" col="6">
						<field name="fromm" colspan="2" />
						<field name="to" colspan="2" />
						<field name="how_many" colspan="2" />
					</group>
				</form>
			</field>
		</record>

	</data>
</openerp>
