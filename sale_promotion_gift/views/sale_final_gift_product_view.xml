<?xml version="1.0"?>
<openerp>
    <data>

        <record id="sale_final_gift_product_tree_view" model="ir.ui.view">
            <field name="name">sale.final.gift.product.tree.view</field>
            <field name="model">sale.final.gift.product</field>
            <field name="arch" type="xml">
                <tree string="Final gift products" editable="bottom">
                    <field name="sale" invisible="1"/>
                    <field name="product"/>
                    <field name="category" />
                    <field name="quantity" />
                </tree>
            </field>
        </record>

    </data>
</openerp>
