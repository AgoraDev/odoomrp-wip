<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
	
		<record model="ir.ui.view" id="view_picking_in_form_inh_qualitycontrolext">
			<field name="name">view.picking.in.form.inh.qualitycontrolext</field>
			<field name="model">stock.picking</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="stock.view_picking_in_form" />
			<field name="arch" type="xml">
				<xpath expr="//page[@string='Notes']" position="after" >
	                <page string="Quality Tests">
						<group colspan="4" col="8">
							<field name="created_tests" colspan="2" />
							<field name="realized_tests" colspan="2" />
							<field name="ok_tests" colspan="2" />
							<field name="nook_tests" colspan="2" />
							<field name="qc_test_ids" nolabel="1" colspan="8" height="300"/>
						</group>
	                </page>
				</xpath>
			</field>
		</record>

	</data>
</openerp>