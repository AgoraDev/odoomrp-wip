<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>
	
		<record model="ir.ui.view" id="qc_test_line_tree_view_inh_qualitycontrolext">
			<field name="name">qc.test.line.tree.view.inh.qualitycontrolext</field>
			<field name="model">qc.test.line</field>
			<field name="type">tree</field>
			<field name="inherit_id" ref="nan_quality_control.qc_test_line_tree_view" />
			<field name="arch" type="xml">
				<field name="proof_id" position= "before">
					<field name="sequence" />
				</field>
			</field>
		</record>
		

		<record model="ir.ui.view" id="qc_test_line_form_view_inh_qualitycontrolext">
			<field name="name">qc.test.line.form.view.inh.qualitycontrolext</field>
			<field name="model">qc.test.line</field>
			<field name="type">form</field>
			<field name="inherit_id" ref="nan_quality_control.qc_test_line_form_view" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='actual_value_qt']/.." position="attributes" >
					<attribute name="col">6</attribute>
				</xpath>
				<xpath expr="//separator[@string='Test Result']" position="attributes" >
					<attribute name="colspan">6</attribute>
				</xpath>
				<xpath expr="//field[@name='actual_value_qt']" position="before" >
					<field name="sequence" />
				</xpath>
			</field>
		</record>
		
	</data>
</openerp>